{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "declaration": false,
    "removeComments": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "newLine": "LF",
    "sourceMap": false,
    "lib": ["ESNext", "DOM", "DOM.Iterable"]
  },
  "include": [
    "src"
  ]
}